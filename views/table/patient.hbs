
<!-- Begin Page Content -->
<div class="container-fluid">

  <!-- Page Heading -->
  <h1 class="h3 mb-2 text-gray-800">Tabla</h1>
  <!-- DataTales Example -->
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Usuarios {{rep.length}}</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>Fecha</th>
              <th>Infectados</th>
              <th>Persona de Alto riesgo</th>
              <th>Infectado de alto riesgo</th>
              <th>Recuperados</th>
            </tr>
          </thead>
          <tfoot>
            <tr>
              <th>Fecha</th>
              <th>Infectados</th>
              <th>Persona de Alto riesgo</th>
              <th>Infectado de alto riesgo</th>
              <th>Recuperados</th>
            </tr>
          </tfoot>
          <tbody>
            {{#each total }}      
            <tr>
                <td>{{ datedb }}:{{timedb}}</td>
                <td>{{ infected }}</td>
                <td>{{ risk_n }}</td>
                <td>{{ risk_y }}</td>
                <td>{{recovered}}</td>
              </tr>
              {{else}}
            {{/each}}

          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
